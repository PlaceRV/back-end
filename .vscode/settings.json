{
	"editor.foldingImportsByDefault": true,
	"explorerExclude.backup": {},
	"files.eol": "\n",
	"files.exclude": {
		"**/*.example*": true,
		"**/*.js": true,
		"**/*spec*": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/CVS": true,
		"**/Thumbs.db": true,
		"**/schema.gql": true,
		"*.json": true,
		".adminjs": true,
		".env": true,
		".git": true,
		".github": true,
		".gitignore": true,
		".prettierrc": true,
		".vscode": true,
		"LICENSE": true,
		"coverage": true,
		"dist": false,
		"nest-cli.json": true,
		"node_modules": true,
		"package-lock.json": true,
		"secrets": true,
		"types.d.ts": false
	},
	"jest.outputConfig": {
		"clearOnRun": "terminal"
	},
	"jest.runMode": {
		"coverage": true,
		"showInlineError": true,
		"type": "on-demand"
	},
	"testing.openTesting": "neverOpen",
	"todohighlight.keywords": [

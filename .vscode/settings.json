{
	"editor.foldingImportsByDefault": true,
	"explorerExclude.backup": {},
	"files.eol": "\n",
	"files.exclude": {
		"**/*.example*": true,
		"**/*.spec*": true,
		"**/.DS_Store": true,
		"**/.adminjs": true,
		"**/.git": true,
		"**/.gitignore": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/CVS": true,
		"**/Thumbs.db": true,
		"**/dist": true,
		"**/node_modules": true,
		"**/package-lock.json": true,
		"**/schema.gql": true,
		".github": true,
		".prettierrc": false,
		".vscode": true,
		"LICENSE": true,
		"coverage": true,
		"nest-cli.json": true,
		"secrets": true
	},
	"jest.outputConfig": {
		"clearOnRun": "terminal"
	},
	"jest.runMode": {
		"coverage": true,
		"showInlineError": true,
		"type": "on-demand"
	},
	"testing.openTesting": "neverOpen",
	"todohighlight.keywords": [
		{
			"backgroundColor": "#00bfff",
			"color": "#4b3621",
			"isWholeLine": true,
			"text": "Queries"
		},
		{
			"backgroundColor": "#69359c",
			"color": "#e3a857",
			"isWholeLine": true,
			"text": "Mutations"
		},
		{
			"backgroundColor": "#E4D6A7",
			"color": "#1C110A",
			"isWholeLine": true,
			"text": "Relationships"
		},
		{
			"backgroundColor": "#CDE7B0",
			"color": "#BE6E46",
			"isWholeLine": true,
			"text": "Basic infomation"
		},
		{
			"backgroundColor": "#FF3366",
			"color": "#011627",
			"isWholeLine": true,
			"text": "Sensitive infomation"
		},
		{
			"backgroundColor": "#4464AD",
			"color": "#FFC759",
			"isWholeLine": true,
			"text": "Methods"
		},
		{
			"backgroundColor": "#420039",
			"color": "#FFC1CF",
			"isWholeLine": true,
			"text": "Database requests"
		}
	]
}
